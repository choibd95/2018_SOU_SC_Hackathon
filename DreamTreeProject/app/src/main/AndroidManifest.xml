            android:screenOrientation="portrait"
            android:launchMode="singleInstance"
            android:excludeFromRecents="true"
            android:taskAffinity=""

            >
        </activity>

        <service
            android:name="com.kimjinhyeok.tree.dreamtreeproject.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service
            android:name="com.kimjinhyeok.tree.dreamtreeproject.MyFirebaseInstanceIDService" >
